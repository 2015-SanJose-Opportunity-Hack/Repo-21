<div class="wrapper debug">

  <div class="span-12-d debug h-10 col  ta-cen flex-center">
    <div class="debug span-3-d col h-3"></div>


    <div class="debug span-6-d col bd-1g ">
      <p class="fs-150">This is the description This is the descriptionThis is the descriptionThis is the descriptionThis is the descriptionThis is the descriptionThis is the descriptionThis is the descriptionThis is the descriptionThis is the description</p>
    </div>

    <div class="debug span-3-d col h-3"> </div>
  </div>

    <!-- start of listings -->
      <div class="col-xs-12 col-sm-6 col-md-8 debug col span-12-d h-45">
        <h1 class="ta-cen">Listing Zones</h1>
        <div class=""><%= link_to 'New Zone', new_zone_path, :class => 'btn btn-primary btn-lg btn-block bg-green' %></div>
        <div class="panel panel-default">
          <div class="panel-body">
            <div class="table-responsive">
        <table class="table">
          <thead>
            <tr>
              <th>Name</th>
              <th>Description</th>
              <th>City</th>
              <th colspan="3"></th>
            </tr>
          </thead>
        <tbody>
          <% @zones.each do |zone| %>
            <tr>
              <td><%= link_to zone.name, addresses_path, :class => 'btn btn-default' %></td>
              <td><%= zone.description %></td>
              <td><%= zone.city %></td>
              <td>
                <%= link_to 'Edit', edit_zone_path(zone), :class => 'btn btn-default' %>
                <%= link_to 'Destroy', zone, :class => 'btn btn-default', method: :delete, data: { confirm: 'Are you sure?' } %>
              </td>
            </tr>
          <% end %>
        </tbody>
        <p id="notice"><%= notice %></p>
      </table>
    </div>
  </div>
</div>
</div>
<!-- zone table end -->






</div>
<!-- wrapper end -->
